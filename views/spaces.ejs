<!DOCTYPE html>
<html lang="en-GB">
<head>
    <%- include("_head", { title: "Spaces" }); %>
</head>
<body>
<h1>Spaces</h1>
<ul>
    <% spaceArray.results.forEach(function(space) { %>
        <li>
            <a href="/space/<%= space.key %>"><h2><%= space.name %></h2></a>
            <p>Key: "<%= space.key %>"</p>
            <p>Type: "<%= space.type %>"</p>
        </li>
    <% }) %>
</ul>
</body>
</html>
